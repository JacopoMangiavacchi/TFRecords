{
    "compilerOptions": {
        "target": "ES2015",
        "module": "commonjs",
        "strict": false,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "declaration": true,
        "outDir": "dist"
    },
    "include": ["src/**/*"],
    "exclude": [
        "node_modules",
        "dist"
    ]
}
